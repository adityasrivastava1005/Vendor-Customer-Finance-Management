    Customer_id VARCHAR(50) NOT NULL PRIMARY KEY,
    Name VARCHAR(50),
    DOB DATETIME,
    Age INT,
    City VARCHAR(50),
    Town VARCHAR(50),
    State VARCHAR(50),
    Vendor_id VARCHAR(50)
);

